# Python API Endpoint Configuration
# Copy this file to .env and update with your actual endpoint URL

# Levi Chat API Endpoint
VITE_PYTHON_API_ENDPOINT=http://az1lxappd01.levi.com:8000/chat

# Account type (default: Revenue)
VITE_ACCOUNT_TYPE=Revenue

# Set to false to use direct REST API calls (recommended)
VITE_USE_GRAPHQL=false

# Option 2: GraphQL Middleware (if you want to use GraphQL)
# Uncomment and set if you want to use GraphQL instead
# VITE_GRAPHQL_ENDPOINT=http://localhost:8000/graphql
# VITE_USE_GRAPHQL=true

# Optional: Authentication token (if your backend requires it)
# VITE_AUTH_TOKEN=your_auth_token_here
